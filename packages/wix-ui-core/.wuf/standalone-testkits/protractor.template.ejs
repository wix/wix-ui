module.exports = {
<%
  components
  .filter(component => component.hasProtractorDriver)
  .map(component => {
    const driverPath = component.protractorDriverPath ? component.protractorDriverPath.replace('dist/', 'dist/standalone/') :
      [
        './../dist/standalone/src/components/',
        ...(component.deprecated ? ['deprecated/'] : []),
        utils.toKebab(component.name),
        '/',
        component.pascalCase ? utils.toPascal(component.name) : component.name,
        '.protractor.driver'
      ].join('');
%>  ...require("<%= driverPath %>"),
<% }) %>
};
