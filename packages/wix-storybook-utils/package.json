{
  "name": "wix-storybook-utils",
  "version": "2.0.1",
  "description": "Utilities for automated component documentation within Storybook",
  "author": {
    "name": "Wix",
    "email": "fed-infra@wix.com"
  },
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "docs",
    "*.js"
  ],
  "bugs": {
    "url": "https://github.com/wix/wix-ui/issues"
  },
  "homepage": "https://github.com/wix/wix-ui#readme",
  "main": "./dist/src/index.js",
  "scripts": {
    "prebuild": "yoshi lint",
    "build": "yoshi build && import-path --path src",
    "pretest": "build-storybook",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "yoshi test --jest",
    "test:e2e": "concurrently 'servedir storybook-static 6006' 'wait-on http://localhost:6006 && cypress run' -k --success first",
    "start": "npm run storybook",
    "release": "yoshi release",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix/wix-ui.git"
  },
  "keywords": [
    "storybook",
    "utilities",
    "documentation",
    "generated",
    "automatic",
    "react"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "^15.4.0"
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "downshift": "^3.1.4",
    "github-markdown-css": "^2.9.0",
    "highlight.js": "9.14.2",
    "loader-utils": "^1.1.0",
    "lodash.merge": "^4.6.1",
    "prettier": "^1.16.2",
    "query-string": "^6.0.0",
    "react-autodocs-utils": "^3.0.0",
    "react-collapse": "^4.0.3",
    "react-docgen": "^2.20.0",
    "react-element-to-jsx-string": "^14.0.2",
    "react-live": "^1.12.0",
    "react-motion": "^0.5.2",
    "react-remarkable": "^1.1.3",
    "react-syntax-highlighter": "^9.0.0",
    "recast": "^0.14.7",
    "wix-ui-icons-common": "^2.0.24"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.17.3",
    "@storybook/addon-options": "4.0.0-alpha.9",
    "@storybook/react": "4.0.0-alpha.9",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^23.3.8",
    "@types/react": "~16.8.1",
    "@types/react-dom": "~16.8.1",
    "concurrently": "^4.1.0",
    "cypress": "^3.2.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.5",
    "eslint-config-wix": "latest",
    "identity-obj-proxy": "^3.0.0",
    "import-path": "latest",
    "lodash.kebabcase": "^4.1.1",
    "node-sass": "^4.9.4",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "~16.4.2",
    "react-dom": "~16.4.2",
    "react-test-renderer": "~16.4.2",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "servedir": "^1.0.1",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3",
    "wait-on": "^3.2.0",
    "wix-ui-test-utils": "^1.0.120",
    "yoshi": "^3.17.0"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "yoshi"
    ]
  },
  "eslintConfig": {
    "extends": "yoshi",
    "env": {
      "jest": true
    },
    "rules": {
      "react/jsx-closing-bracket-location": [
        "error",
        {
          "nonEmpty": "tag-aligned",
          "selfClosing": false
        }
      ],
      "react/jsx-handler-names": 0,
      "react/no-find-dom-node": 0,
      "react/no-string-refs": 0,
      "react/jsx-boolean-value": 2,
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "wix-ui-test-utils/jest-setup",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/enzyme-setup"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "/src/.*\\.(spec|test)\\.([tj]sx?)$"
  },
  "yoshi": {
    "servers": {
      "cdn": {
        "port": 3201
      }
    }
  }
}
