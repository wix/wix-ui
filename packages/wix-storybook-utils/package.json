{
  "name": "wix-storybook-utils",
  "version": "0.0.1",
  "description": "Utilities for automated component documentation within Storybook",
  "author": "Arijus Å ukys <argshook@gmail.com> (http://arijus.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix/wix-ui/issues"
  },
  "homepage": "https://github.com/wix/wix-ui#readme",
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "yoshi build",
    "prepush": "yoshi lint",
    "start": "yoshi start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix/wix-ui.git"
  },
  "keywords": [
    "storybook",
    "utilities",
    "documentation",
    "generated",
    "automatic",
    "react"
  ],
  "peerDependencies": {
    "react": "^15.4.0"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.0.8",
    "eslint-config-wix": "^1.1.16",
    "github-markdown-css": "^2.9.0",
    "highlight.js": "^9.12.0",
    "react-docgen": "^2.20.0",
    "react-element-to-jsx-string": "^13.1.0",
    "react-remarkable": "^1.1.3",
    "wix-style-react": "^1.1.4264"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.8.0",
    "husky": "^0.13.4",
    "identity-obj-proxy": "^3.0.0",
    "prop-types": "^15.5.4",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "yoshi": "latest"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "wix/react",
    "env": {
      "jest": true
    },
    "rules": {
      "react/jsx-handler-names": 0,
      "react/no-find-dom-node": 0,
      "react/no-string-refs": 0,
      "react/jsx-boolean-value": 2,
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  },
  "yoshi": {
    "entry": {
      "app": "index.js"
    },
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM"
    },
    "externalUnprocessedModules": [
      "get-own-enumerable-property-symbols",
      "stringify-object"
    ]
  }
}
