{
  "name": "wix-storybook-utils",
  "version": "4.0.0",
  "description": "Utilities for automated component documentation within Storybook",
  "author": {
    "name": "Wix",
    "email": "fed-infra@wix.com"
  },
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "docs",
    "*.js"
  ],
  "bugs": {
    "url": "https://github.com/wix/wix-ui/issues"
  },
  "homepage": "https://github.com/wix/wix-ui#readme",
  "main": "./dist/src/index.js",
  "module": "./dist/es/src/index.js",
  "scripts": {
    "lint": "yoshi lint",
    "build": "yoshi build && import-path --path src",
    "postbuild": "npm run docs:sections",
    "pretest": "npm run lint && build-storybook",
    "test": "yoshi test --jest",
    "start": "start-storybook -p 6006",
    "release": "yoshi release",
    "docs:sections": "node docs/make-sections-docs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix/wix-ui.git"
  },
  "keywords": [
    "storybook",
    "utilities",
    "documentation",
    "generated",
    "automatic",
    "react"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "^15.4.0"
  },
  "dependencies": {
    "@babel/core": "~7.11.0",
    "@babel/generator": "~7.11.0",
    "@babel/parser": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.13.0",
    "@babel/traverse": "~7.11.0",
    "@babel/types": "~7.11.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "babel-runtime": "^6.25.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "downshift": "^3.1.4",
    "ejs": "^2.7.1",
    "github-markdown-css": "^2.9.0",
    "highlight.js": "9.14.2",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.15",
    "prettier": "^1.16.2",
    "query-string": "^6.0.0",
    "react-autodocs-utils": "^3.5.0",
    "react-collapse": "^4.0.3",
    "react-element-to-jsx-string": "^14.0.2",
    "react-live": "^2.2.1",
    "react-motion": "^0.5.2",
    "react-remarkable": "^1.1.3",
    "react-syntax-highlighter": "^9.0.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.17.3",
    "@storybook/addon-options": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@types/jest": "^23.3.8",
    "@types/node": "^12.12.22",
    "@types/react": "~16.8.1",
    "@types/react-dom": "~16.8.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.5",
    "identity-obj-proxy": "^3.0.0",
    "import-path": "latest",
    "node-sass": "^4.9.4",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.2.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.1.3",
    "wix-ui-test-utils": "^1.0.120",
    "yoshi": "^4.16.4",
    "yoshi-style-dependencies": "^4.11.1"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "wix-ui-test-utils/jest-setup"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/enzyme-setup"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "/src/.*\\.(spec|test)\\.([tj]sx?)$"
  }
}
