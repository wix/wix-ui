{
  "private": false,
  "name": "wix-ui-framework",
  "bin": {
    "wuf": "./bin/wuf.js"
  },
  "version": "3.5.2",
  "author": {
    "name": "Wix",
    "email": "fed-infra@wix.com"
  },
  "files": [
    "dist",
    "src",
    "bin",
    "*.md",
    "*.json"
  ],
  "bugs": {
    "url": "https://github.com/wix/wix-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix/wix-ui.git"
  },
  "scripts": {
    "build": "tsc",
    "pretest": "yoshi lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "yoshi lint",
    "postlint": "node scripts/make-readme.js > README.md",
    "start": "tsc --watch",
    "wuf": "./bin/wuf.js"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^2.20.0",
    "dependency-tree": "^8.0.0",
    "ejs": "^2.6.2",
    "globby": "^11.0.3",
    "jscodeshift": "^0.6.4",
    "lodash": "^4.17.11",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "ora": "^3.4.0",
    "prompts": "^2.0.4",
    "simple-git": "^1.110.0"
  },
  "jest": {
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/test-generated/",
      "<rootDir>/src/cli-commands/generate/__testfixtures__"
    ]
  },
  "devDependencies": {
    "@types/dependency-tree": "^8.0.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/webpack": "^5.28.0",
    "@wix/yoshi": "^4.3.0",
    "cista": "0.0.3",
    "jest": "^24.7.1",
    "nixt": "^0.5.1",
    "ts-jest": "^24.0.2",
    "tslint-config-yoshi": "^4.1.0",
    "typescript": "^3.4.5"
  },
  "eslintConfig": {
    "extends": "yoshi",
    "env": {
      "jest": true
    }
  }
}
